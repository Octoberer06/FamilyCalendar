{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "AuthorizationOptions": {
    "RolePermissions": [
      {
        "Role": "Administrator",
        "Permission": [ "Create", "Read", "Update", "Delete" ]
      },
      {
        "Role": "User",
        "Permission": [ "Read" ]
      }
    ]
  }
}
